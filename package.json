{
	"name": "badgr-ui",
	"version": "2.19.3",
	"description": "Badgr UI",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"start-intellij": "ng serve --host 0.0.0.0 --disable-host-check --aot 2>&1 | perl -e '$|++; while (<>) { s/((ERROR|WARNING) in\\s*)(.*)/$1\\n$3/g; print $_; }'",
		"build": "ng build --output-hashing=all --aot --prod && html-minifier -c html-minifier.config.json -o dist/index.html < dist/index.html",
		"test": "ng test",
		"test-intellij": "ng test 2>&1 | perl -e '$|++; while (<>) { s/((ERROR|WARNING) in\\s*)(.*)/$1\\n$3/g; print $_; }'",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"version": "npm run build",
		"postversion": "git push && git push --tags"
	},
	"private": true,
	"license": "ISC",
