    "reflect-metadata": "0.1.10",
    "rxjs": "^6.3.3",
    "sanitize-html": "^1.19.2",
    "systemjs": "0.20.12",
    "tether-tooltip": "^1.2.0",
    "tiny-sha256": "1.0.2",
    "tslib": "^1.9.0",
    "validator": "^9.0.0",
    "zone.js": "^0.8.26"
