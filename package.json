{
  "name": "badgr-ui",
  "version": "2.19.3",
  "description": "Badgr UI",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-intellij": "ng serve --host 0.0.0.0 --disable-host-check --aot 2>&1 | perl -e '$|++; while (<>) { s/(ERROR|WARNING in\\s*)(.*)/$1\\n$2/g; print $_; }'",
    "build": "ng build --output-hashing=all --aot --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "css:build": "npm run css:clean && npm run css:build:sass && npm run css:build:assets && npm run css:copy:js",
    "css:build:assets": "npm run css:imagemin && npm run css:fontmin",
    "css:build:sass": "npm run css:node-sass && npm run css:postcss",
    "css:clean": "npm run rimraf -- dist/styleguide && npm run rimraf -- temp",
    "css:copy:js": "cp -r src/breakdown/static/js temp",
    "css:dev": "npm run css:build && fractal start --sync",
    "css:prod": "npm run css:build && fractal build",
    "css:fontmin": "fontmin src/breakdown/static/scss/fonts/* temp/fonts",
    "css:imagemin": "imagemin src/breakdown/static/scss/images/* -o temp/images",
    "css:node-sass": "node-sass -o temp/ src/breakdown/static/scss/theme-default.scss",
    "css:postcss": "postcss -u autoprefixer -r temp/theme-default.css",
    "css:watch": "onchange 'src/breakdown/static/scss/**/*.scss' -- npm run css:build:sass"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "7.1.1",
    "@angular/cli": "7.1.0",
    "@angular/common": "7.1.1",
    "@angular/compiler": "7.1.1",
    "@angular/compiler-cli": "7.1.1",
    "@angular/core": "7.1.1",
    "@angular/forms": "7.1.1",
    "@angular/http": "7.1.1",
    "@angular/platform-browser": "7.1.1",
    "@angular/platform-browser-dynamic": "7.1.1",
    "@angular/router": "7.1.1",
    "@angularclass/request-idle-callback": "1.0.7",
    "@types/fontfaceobserver": "0.0.5",
    "@types/karma-jasmine": "0.0.29",
    "@types/marked": "0.0.28",
    "@types/validator": "^6.3.0",
    "angulartics2": "^7.1.0",
    "base64-image-utils": "0.2.0",
    "clipboard": "1.6.1",
    "core-js": "^2.4.1",
    "css-element-queries": "^0.4.0",
    "dialog-polyfill": "0.4.7",
    "file-saver": "^1.3.8",
    "font-awesome": "4.7.0",
    "fontfaceobserver": "^2.0.9",
    "handlebars-helpers": "^0.8.2",
    "ie-shim": "^0.1.0",
    "intl": "1.2.5",
